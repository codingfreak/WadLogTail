<Application x:Class="codingfreaks.WadLogTail.Ui.WindowsApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:codingfreaks.WadLogTail.Ui.WindowsApp"
             StartupUri="MainWindow.xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             d1p1:Ignorable="d"
             xmlns:own="clr-namespace:codingfreaks.WadLogTail.Ui.WindowsApp"
             xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006">
    <Application.Resources>
        <own:MessageListener x:Key="MessageListener" />
        <vm:ViewModelLocator x:Key="Locator" d:IsDataSource="True"
                                 xmlns:vm="clr-namespace:codingfreaks.WadLogTail.Ui.WindowsApp.ViewModel" />
    </Application.Resources>
</Application>